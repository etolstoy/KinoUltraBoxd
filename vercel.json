{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "KinoUltraBoxd/services/**",
          "KinoUltraBoxd/models/**",
          "KinoUltraBoxd/imdb.sqlite"
        ]
      }
    }
  ],
  "routes": [
    { "src": "/api/webhook", "dest": "api/webhook.ts" }
  ]
} 